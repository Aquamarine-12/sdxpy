<!DOCTYPE html>
<html lang="en">
  {% include "head.html" %}
  <body>
    <div class="row">
      <div class="sidebar">
        {% include "contents.html" %}
      </div>
      <div id="printable" class="contents bordered">
        <main>
          {% include "title.html" %}
          {% if node.slug in site.todo %}
          <div class="todo">
            <p>TO DO</p>
          </div>
          {% else %}
            {% include "draft.html" %}
            {% include "syllabus.html" %}
            {% include "definitions.html" %}
            <div class="page-toc"></div>
            {{ node.html }}
          {% endif %}
        </main>
      </div>
    </div>
  </body>
</html>
